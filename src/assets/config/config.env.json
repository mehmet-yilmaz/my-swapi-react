{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "categories": {
        "people": {
            "headerField": "name",
            "mapFields": [
                "birth_year",
                "eye_color",
                "gender",
                "hair_color",
                "height",
                "mass",
                "skin_color"
            ]
        },
        "films": {
            "headerField": "title",
            "mapFields": [
                "episode_id", 
                "director", 
                "producer", 
                "release_date"
            ]
        },
        "planets": {
            "headerField": "name",
            "mapFields": [
                "diameter",
                "rotation_period",
                "orbital_period",
                "gravity",
                "population",
                "climate"
            ]
        },
        "species": {
            "headerField": "name",
            "mapFields": [
                "classification",
                "designation",
                "average_height",
                "average_lifespan",
                "eye_colors",
                "hair_colors",
                "skin_colors",
                "language"
            ]
        },
        "starships": {
            "headerField": "name",
            "mapFields": [
                "model",
                "starship_class",
                "manufacturer",
                "cost_in_credits",
                "length",
                "crew",
                "passengers",
                "max_atmosphering_speed"
            ]
        },
        "vehicles": {
            "headerField": "name",
            "mapFields": [
                "model",
                "vehicle_class",
                "manufacturer",
                "length",
                "cost_in_credits",
                "crew",
                "passengers",
                "max_atmosphering_speed"
            ]
        }
    }
}